{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Todo",
  "description": "Todo Schema",
  "required": [
    "id",
    "title"
  ],
  "version": 0,
  "properties": {
    "id": {
      "type": "string",
      "primary": true,
      "pattern": "^(.*)$"
    },
    "projectId": {
      "type": "string",
      "index": true
    },
    "parentId": {
      "type": "string"
    },
    "reminderId": {
      "type": "string"
    },
    "repeatCfgId": {
      "type": "string"
    },
    "created": {
      "type": "number",
      "index": true
    },
    "title": {
      "type": "string"
    },
    "subTaskIds": {
      "type": "array",
      "uniqueItems": true,
      "items": {
        "type": "string"
      }
    },
    "timeSpent": {
      "type": "number"
    },
    "timeSpentOnDay": {
      "type": "object"
    },
    "timeEstimate": {
      "type": "number"
    },
    "isDone": {
      "type": "boolean"
    },
    "notes": {
      "type": "string"
    },
    "issueId": {
      "type": "string"
    },
    "issueType": {
      "type": "string"
    },
    "attachmentIds": {
      "type": "array",
      "uniqueItems": true,
      "items": {
        "type": "string"
      }
    },
    "ui_isAdditionalInfoOpen": {
      "type": "boolean"
    },
    "ui_showSubTasksMode": {
      "type": "number"
    },
    "ui_currentTab": {
      "type": "number"
    }
  }
}
